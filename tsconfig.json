{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ScriptHost",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "client/src/*"
      ],
      "@components/*": [
        "client/src/components/*"
      ],
      "@lib/*": [
        "client/src/lib/*"
      ],
      "@pages/*": [
        "client/src/pages/*"
      ]
    }
  },
  "include": [
    "client/src"
  ],
  "exclude": [
    "node_modules"
  ]
}